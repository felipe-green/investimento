{
  "compilerOptions": {
    "target": "es6", // Material UI requirements
    "lib": [
      "es6", // Material UI requirements
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true, // Material UI requirements
    "noImplicitThis": true, // Material UI requirements
    "strictNullChecks": true, // Material UI requirements
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
